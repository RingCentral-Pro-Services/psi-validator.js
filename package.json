{
  "name": "psi-validator",
  "version": "0.0.1",
  "description": "This will provide a validator class for many different RC API endpoints",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*",
    "README.md"
  ],
  "keywords": [
    "validator",
    "validation",
    "validate",
    "sanitization",
    "sanitize",
    "sanitisation",
    "sanitise",
    "assert"
  ],
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RingCentral-Pro-Services/psi-validator.js.git"
  },
  "homepage": "https://github.com/RingCentral-Pro-Services/psi-validator.js",
  "bugs": {
    "url": "https://github.com/RingCentral-Pro-Services/psi-validator.js/issues"
  },
  "author": "John Celoria <jceloria@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@tsconfig/node14": "^1.0.0",
    "@types/jest": "^26.0.20",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "typescript": "^4.2.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 75,
        "functions": 80,
        "lines": 80,
        "statements": -10
      }
    }
  }
}
